    <None Remove="Wood Wall And Wood Floor With Some Notebooks Photography Backdrop.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="images\HarryPoterSecretsRoom.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignColors" Version="2.0.9" />
    <PackageReference Include="MaterialDesignThemes" Version="4.6.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BL\BL.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="images\Wood Wall And Wood Floor With Some Notebooks Photography Backdrop.jpg" />
    <Resource Include="Wood Wall And Wood Floor With Some Notebooks Photography Backdrop.jpg" />
  </ItemGroup>

</Project>
